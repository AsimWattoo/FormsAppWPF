<Page x:Class="FormsApp.Controls.Navigator"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:FormsApp.Controls"
      xmlns:at="clr-namespace:FormsApp.Attached_Properties"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Navigator">

    <Grid>
        <Frame x:Name="OldContentFrame" at:NoFrameHistory.Value="True"/>
        <Frame x:Name="NewContentFrame" at:NoFrameHistory.Value="True"/>
    </Grid>
</Page>
